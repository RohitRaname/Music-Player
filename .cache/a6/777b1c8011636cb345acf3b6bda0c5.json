}},{"generated":{"line":129,"column":11},"source":"../view/CrudView.js","original":{"line":74,"column":40}},{"generated":{"line":129,"column":12},"source":"../view/CrudView.js","original":{"line":88,"column":8}},{"generated":{"line":130,"column":0},"source":"../view/CrudView.js","original":{"line":90,"column":8}},{"generated":{"line":132,"column":0},"source":"../view/CrudView.js","original":{"line":91,"column":7}},{"generated":{"line":133,"column":0},"source":"../view/CrudView.js","original":{"line":92,"column":5}},{"generated":{"line":133,"column":7},"source":"../view/CrudView.js","original":{"line":21,"column":4}},{"generated":{"line":134,"column":0},"source":"../view/CrudView.js","original":{"line":93,"column":3}},{"name":"ParentView","generated":{"line":138,"column":2},"source":"../view/CrudView.js","original":{"line":10,"column":23}},{"name":"CrudView","generated":{"line":140,"column":15},"source":"../view/CrudView.js","original":{"line":96,"column":15}}],"sources":{"../view/CrudView.js":"import ParentView from \"./ParentView.js\";\nimport {\n  closest,\n  contains,\n  addClass,\n  removeClass,\n  toggleClass,\n} from \"../utils/_domFunction.js\";\n\nclass CrudView extends ParentView {\n  _parentEl = document.querySelector(\".crud\");\n  _cardList = document.querySelector(\".card__list\");\n  _cardElArr = [...document.querySelectorAll(\".card\")];\n\n  constructor() {\n    super();\n    this.handleCrud();\n  }\n\n  handleCrud() {\n    this._parentEl.addEventListener(\"click\", (e) => {\n      let target = e.target;\n\n      // select button---------------------------------------\n      if (closest(target, \"card__control\")) {\n        target = closest(target, \"card__control\");\n\n        toggleClass(target, \"card__control--active\");\n\n        if (contains(target, \"card__control--active\")) {\n          this._cardElArr.forEach((mov) =>\n            mov\n              .querySelector('.card__control[data-action=\"select\"]')\n              .classList.add(\"card__control--active\")\n          );\n          // card control select ALL as active\n\n          // update the selected card count\n\n          this.updateSelectedCardsCount();\n        } else {\n          removeClass(this._cardList, \"card__list--select\");\n\n          this._cardElArr.forEach((mov) =>\n            mov\n              .querySelector('.card__control[data-action=\"select\"]')\n              .classList.remove(\"card__control--active\")\n          );\n\n          addClass(this._parentEl, \"hide\");\n          this.updateSelectedCardsCount();\n        }\n      }\n\n      // to go back to normal home page ------------------------------------------\n      if (closest(target, \"crud-clear\")) {\n        target = closest(target, \"crud-clear\");\n        addClass(this._parentEl, \"hide\");\n\n        // hide select element\n        removeClass(this._cardList, \"card__list--select\");\n\n        // remove select element active class\n        this._cardElArr.forEach((mov) => {\n          mov\n            .querySelector('.card__control[data-action=\"select\"]')\n            .classList.remove(\"card__control--active\");\n        });\n\n        this.updateSelectedCardsCount();\n      }\n\n      // music btns----------------------------------\n      if (closest(target, \"crud__btn\")) {\n        target = target.closest(\".crud__btn\");\n        const { action } = target.dataset;\n\n        // Removing cards dom\n        if (action === \"remove\") {\n          this._cardElArr.forEach((mov) => {\n            if (\n              mov.querySelector('.card__control--active[data-action=\"select\"]')\n            )\n              mov.remove();\n          });\n        }\n\n        // play music\n\n        // add to playlist\n      }\n    });\n  }\n}\n\nexport default CrudView;\n"},"lineCount":null}},"error":null,"hash":"f93d14a3269ffc7108a07e0e0470e44a","cacheData":{"env":{}}}