(\"loadedmetadata\", function () {\n    console.log(audio.duration);\n    state.duration = audio.duration;\n    durationEl.textContent = formatTime(state.duration);\n    audio.play();\n  });\n}\n\n// Update time and progressbar movement\nfunction updateTimeAndProgressBarEl() {\n  audio.addEventListener(\"timeupdate\", function () {\n    curTimeEl.textContent = formatTime(audio.currentTime);\n    state.currentTime = +audio.currentTime;\n\n    const percent = Math.floor(\n      (Number(audio.currentTime) / Number(audio.duration)) * 100\n    );\n\n    progressBar.style.width = `${percent}%`;\n  });\n}\n\nfunction updateProgressBar(percent) {\n  // parent.addEventListener(\"click\", function (e) {\n  //   const target = e.target;\n  //   // respect to target\n  //   const clickWidthFromStart = +target.offsetX;\n  //   const percent = Math.floor((clickWidthFromStart / progressWidth) * 100);\n  //   child.style.width = `${percent}%`\n  // });\n}\n\n// prev song -> loadsong\nfunction prevSong() {\n  let index = state.index;\n  index -= index;\n  if (index < 0) index = state.songs.length - 1;\n\n  state.index = index;\n\n  loadSong(index);\n}\n\n// next song -> loadsong\nfunction nextSong() {\n  let index = state.index;\n  index += index;\n  if (index > state.songs.length - 1) index = 0;\n\n  state.index = index;\n\n  loadSong(index);\n}\n\n// load song (play song)\nfunction loadSong(index) {\n  if (!index) index = state.index;\n\n  // set basic el\n  const song = state.songs[index];\n  audio.src = `./music/${song.music}`;\n  imgEl.src = `./img/${song.img}`;\n  titleEl.textContent = song.name;\n  artistEl.textContent = song.author;\n\n  // play song and set duration of song\n  playWhenLoadedAndSetDuration();\n\n  //   set current time ,duration and run progressBar\n  updateTimeAndProgressBarEl();\n}\n\nsectionControl.addEventListener(\"click\", (e) => {\n  let target = e.target;\n});\n\nexport default loadSong;\n"},"lineCount":null}},"error":null,"hash":"b636ec148627ca140b65e1cc502ef2af","cacheData":{"env":{}}}