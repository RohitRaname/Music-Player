wPlaylistName = this._getInputName();\n    if (\n      !newPlaylistName ||\n      newPlaylistName === \"\" ||\n      newPlaylistName === undefined\n    )\n      return;\n\n    handle(\"rename\", newPlaylistName);\n    this._clearInputName();\n    this._hideModal();\n  }\n\n  _handleCancelBtn(target) {\n    // check target  is right\n    if (!closest(target, \"playlistSongrename-modal-btn[data-action='cancel']\"))\n      return;\n\n    this._clearInputName();\n    this._hideModal();\n  }\n\n  _handleDeleteBtn(target, handle) {}\n\n  addHandlerRenamePlaylist(handle) {\n    for (const modal of this._modals) {\n      modal.addEventListener(\"click\", (e) => {\n        const target = e.target;\n\n        // handle input && handle rename\n        this._handleRenameBtn(target, handle);\n\n        this._handleDeleteBtn(target, handle);\n\n        // handle cancel\n        this._handleCancelBtn(target);\n      });\n    }\n  }\n}\n\nexport default new ModalView();\n"},"lineCount":null}},"error":null,"hash":"4c9538c205d702c433dbeb604305656d","cacheData":{"env":{}}}